; xin standard library

; boolean aliases
(: true 1)
(: false 0)

; basic io
(: (log s)
   (<- os::stdout (+ (string s) '\n')))
(: (scan)
   (-> os::stdin))

; iteration primitives: range, times
(: (range start end step)
   (do
     (: (sub i acc)
        (if (> i end)
          acc
          (do (vec-add! acc i)
            (sub (+ i step) acc))))
     (sub start (vec))))

(: (xrange n)
   (range 0 n 1))

